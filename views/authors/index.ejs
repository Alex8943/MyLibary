<h2>Search author</h2>
<form action="/authors" method="GET">
    <label>Name</label>
    <% if(searchOptions.name != null) { %>
        <input type="text" name="name" value="<%= searchOptions.name %>">
    <% } %>
    <button type="submit">Search</button>
</form>
<br> 
<% authors.forEach(author => { %>
    <div><%= author.name %></div>
<% }) %>